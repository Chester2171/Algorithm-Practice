namespace IntArrayTimesLength
{
    class Program
    {
        public static int[] MultipleLength(int[] input)
        {
            int[] output = new int[input.Length];
            
            for (int i = 0; i < input.Length; i++)
            {
                output[i] = input[i] * input.Length;
            }
                return output;
        }

        static void Main(string[] args)
        {
            int[] input = new int[] { 5, 19, 21, -5 };
            MultipleLength(input);
        }
    }
}
